<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WhereBackEnd</title>

    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="plugins/images/favicon.png">
    <!-- Bootstrap Core CSS -->
    <link href="~/SiteStyle/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/SiteStyle/plugins/select2/select2-bootstrap.min.css" rel="stylesheet" />
    <link href="~/SiteStyle/plugins/select2/select2.min.css" rel="stylesheet" />
    <link href="//cdn.datatables.net/1.10.22/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/SiteStyle/plugins/date/bootstrap-material-datetimepicker.css" rel="stylesheet" />
    <!-- animation CSS -->
    <link href="~/SiteStyle/css/animate.css" rel="stylesheet">
    <link href="~/SiteStyle/css/fontawesome/all.min.css" rel="stylesheet" />
    <!-- Custom CSS -->
    <link href="~/SiteStyle/css/style.css" rel="stylesheet">
    <!-- color CSS -->
    <link href="~/SiteStyle/css/colors/default.css" id="theme" rel="stylesheet">
    <link href="~/SiteStyle/css/main.css" rel="stylesheet">
    @*<link href="~/SiteStyle/css/Language/English/English.css" rel="stylesheet" />*@
    <link href="~/SiteStyle/css/Language/Arabic/Arabic.css" rel="stylesheet" />
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/SiteStyle/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/SiteStyle/plugins/charts/highcharts.js"></script>
    <script src="~/SiteStyle/plugins/charts/exporting.js"></script>
    <script src="~/SiteStyle/plugins/charts/export-data.js"></script>
    <script src="~/SiteStyle/plugins/charts/accessibility.js"></script>


</head>
<body class="fix-header">
    <!-- ============================================================== -->
    <!-- Preloader -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Wrapper -->
    <!-- ============================================================== -->
    <div id="wrapper">
        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        <nav class="navbar navbar-default navbar-static-top m-b-0">
            <div class="navbar-header">
                <div class="top-left-part">
                    <!-- Logo -->
                    <a class="logo" href="dashboard.html">
                        <!-- Logo icon image, you can use font-icon also --><b>
                            <!--This is dark logo icon-->
                            <img src="~/SiteStyle/plugins/images/Logo.png" alt="home" class="dark-logo" />
                        </b>
                        <!-- Logo text image you can use text also --><span class="hidden-xs">
                            <!--This is light logo text--><img src="~/SiteStyle/plugins/images/Logo.png" alt="home" class="light-logo" />
                        </span>
                    </a>
                </div>
                <!-- /Logo -->
                <partial name="~/Views/Shared/_NavBar.cshtml" />
            </div>
            <!-- /.navbar-header -->
            <!-- /.navbar-top-links -->
            <!-- /.navbar-static-side -->
        </nav>
        <!-- End Top Navigation -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <partial name="~/Views/Shared/_SideBar.cshtml" />
        <div id="page-wrapper">
            @RenderBody()
        </div>
    </div>
    <!-- Modal -->
    <div class="modal fade" id="EditModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">تعديل الصنف</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="row">
                            <div class="col-md-6 form-group">
                                <div class="row m-0-l-r">
                                    <label class="col-md-4">from</label>
                                    <div class="col-md-7">
                                        <input class="form-control" type="date">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 form-group">
                                <div class="row m-0-l-r">
                                    <label class="col-md-offset-1 col-md-4">to</label>
                                    <div class="col-md-7">
                                        <input class="form-control" type="date">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 form-group">
                                <div class="row m-0-l-r">
                                    <label class="col-md-4">id</label>
                                    <div class="col-md-7">
                                        <input class="form-control" value="">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 form-group">
                                <div class="row m-0-l-r">
                                    <label class="col-md-offset-1 col-md-4">name</label>
                                    <div class="col-md-7">
                                        <input class="form-control" value="">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 form-group">
                                <div class="row m-0-l-r">
                                    <label class="col-md-4">Still</label>
                                    <div class="col-md-7">
                                        <select class="select2 form-control">
                                            <option>Still</option>
                                            <option>no</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 form-group">
                                <div class="row m-0-l-r">
                                    <label class="col-md-offset-1 col-md-4">
                                        The type of disease
                                    </label>
                                    <div class="col-md-7">
                                        <select class="select2 form-control">
                                            <option>Heart patient</option>
                                            <option>diabetic</option>
                                            <option>Pressure patient</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row form-group m-0-l-r">
                            <div class="col-md-3 col-md-offset-7">
                                <a class="SearchForm pull-right Edit" onclick="NormalSearch(this)">Normal Search</a>
                            </div>
                            <div class="col-md-2">
                                <button class="SearchForm pull-right">Search</button>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save</button>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="DeleteModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabelDelete" aria-hidden="true">
        <div class="modal-dialog modal-sm" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabelDelete">حذف الصنف</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12 form-group text-center">
                            <h3>هل انت متأكد من حذف الصنف ؟</h3>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-danger">Delete</button>
                </div>
            </div>
        </div>
    </div>
    <p class="text-center Footer">All Copyrights reserved to <a href="#">Wasmah</a> © 2020</p>
    <script src="//cdn.datatables.net/1.10.22/js/jquery.dataTables.min.js"></script>
    <script src="~/SiteStyle/plugins/select2/select2.min.js"></script>
    <script src="~/SiteStyle/plugins/select2/select2.full.min.js"></script>
    <script src="~/SiteStyle/js/custom.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
