
@{
    ViewData["Title"] = "SearchPage";
}

<div class="row bg-title m-0-l-r">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h4 class="page-title">Search</h4>
    </div>
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">

        <ol class="breadcrumb">
            <li><a href="#">Home</a></li>
            <li class="active">Search</li>
        </ol>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /row -->
<div class="row">
    <div class="col-sm-12">
        <div class="white-box NormalSearch">
            <form>
                <div class="row">
                    <div class="col-md-12 form-group">
                        <div class="row m-0-l-r">
                            <label class="col-md-2">id</label>
                            <div class="col-md-10">
                                <input class="form-control" value="">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row form-group m-0-l-r">
                    <div class="col-md-2 col-md-offset-8">
                        <a class="SearchForm pull-right Edit" onclick="AdvancedSearch(this)">Advanced Search</a>
                    </div>
                    <div class="col-md-2">
                        <button class="SearchForm pull-right">Search</button>
                    </div>
                </div>
            </form>
        </div>
        <div class="white-box AdvancedSearch">
            <form>
                <div class="row">
                    <div class="col-md-6 form-group">
                        <div class="row m-0-l-r">
                            <label class="col-md-4">من</label>
                            <div class="col-md-7">
                                <input class="form-control" type="date">
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <div class="row m-0-l-r">
                            <label class="col-md-offset-1 col-md-4">الى</label>
                            <div class="col-md-7">
                                <input class="form-control" type="date">
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <div class="row m-0-l-r">
                            <label class="col-md-4">id</label>
                            <div class="col-md-7">
                                <input class="form-control" value="">
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <div class="row m-0-l-r">
                            <label class="col-md-offset-1 col-md-4">name</label>
                            <div class="col-md-7">
                                <input class="form-control" value="">
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <div class="row m-0-l-r">
                            <label class="col-md-4">active</label>
                            <div class="col-md-7">
                                <select class="select2 form-control">
                                    <option>active</option>
                                    <option>not active</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 form-group">
                        <div class="row m-0-l-r">
                            <label class="col-md-offset-1 col-md-4">Descripton</label>
                            <div class="col-md-7">
                                <input class="form-control" value="">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row form-group m-0-l-r">
                    <div class="col-md-2 col-md-offset-8">
                        <a class="SearchForm pull-right Edit" onclick="NormalSearch(this)">Normal Search</a>
                    </div>
                    <div class="col-md-2">
                        <button class="SearchForm pull-right">Search</button>
                    </div>
                </div>
            </form>
        </div>
